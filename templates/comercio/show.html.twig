{% extends 'base.html.twig' %}

{% block title %}Comercio{% endblock %}

{% block body %}
    <h1>Comercio</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ comercio.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ comercio.nombre }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ comercio.direccion }}</td>
            </tr>
            <tr>
                <th>Latitud</th>
                <td>{{ comercio.latitud }}</td>
            </tr>
            <tr>
                <th>Longitud</th>
                <td>{{ comercio.longitud }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ comercio.observacion }}</td>
            </tr>
            <tr>
                <th>NombreLogo</th>
                <td>{{ comercio.nombreLogo }}</td>
            </tr>
            <tr>
                <th>LinkGoogleMaps</th>
                <td>{{ comercio.linkGoogleMaps }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ comercio.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>FechaCreacion</th>
                <td>{{ comercio.fechaCreacion ? comercio.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaActualizacion</th>
                <td>{{ comercio.fechaActualizacion ? comercio.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('comercio_index') }}">back to list</a>

    <a href="{{ path('comercio_edit', {'id': comercio.id}) }}">edit</a>

    {{ include('comercio/_delete_form.html.twig') }}
{% endblock %}
