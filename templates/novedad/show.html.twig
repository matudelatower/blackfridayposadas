{% extends 'base.html.twig' %}

{% block title %}Novedad{% endblock %}

{% block body %}
    <h1>Novedad</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ novedad.id }}</td>
            </tr>
            <tr>
                <th>Titulo</th>
                <td>{{ novedad.titulo }}</td>
            </tr>
            <tr>
                <th>Cuerpo</th>
                <td>{{ novedad.cuerpo }}</td>
            </tr>
            <tr>
                <th>NombreImagenDestacada</th>
                <td>{{ novedad.nombreImagenDestacada }}</td>
            </tr>
            <tr>
                <th>Activo</th>
                <td>{{ novedad.activo ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>FechaCreacion</th>
                <td>{{ novedad.fechaCreacion ? novedad.fechaCreacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>FechaActualizacion</th>
                <td>{{ novedad.fechaActualizacion ? novedad.fechaActualizacion|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('novedad_index') }}">back to list</a>

    <a href="{{ path('novedad_edit', {'id': novedad.id}) }}">edit</a>

{#    {{ include('novedad/_delete_form.html.twig') }}#}
{% endblock %}
